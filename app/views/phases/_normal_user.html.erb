<%- model_class = Phase -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:name) %></th>
    </tr>
  </thead>
  <tbody>
    <% @phases.each do |phase| %>
      <tr>
        <td><%= phase.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>


